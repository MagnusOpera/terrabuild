[<export>]
let run (context: {| Command: string |}) =
    let scriptName =
        match Env.ScriptName with
        | Some name -> name
        | None -> "<none>"

    $"{scriptName}|{Env.Arguments |> List.length}"

type ExportFlag =
  | Dispatch
  | Default
  | Never
  | Local
  | External
  | Remote

{
  [nameof run] = []
}
